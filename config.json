{
  "listeners": {
    "127.0.0.1:7474": {
      "pass": "applications/now"
    }
  },
  "applications": {
    "now": {
      "type": "external",
      "working_directory": "~/.now",
      "executable": "/usr/local/bin/now",
      "processes": 1,
      "limits": {
        "timeout": 1
      }
    }
  }
}